{
  "haste": {
    "defaultPlatform": "ios",
    "platforms": ["android", "ios", "native"],
    "hasteImplModulePath": "<rootDir>/node_modules/react-native/jest/hasteImpl.js",
    "providesModuleNodeModules": ["react-native"]
  },
  "moduleNameMapper": {
    "^React$": "<rootDir>/node_modules/react",
    "\\.(css|less)$": "<rootDir>/node_modules/@youi/react-native-youi/__mocks__/styleMock.js"
  },
  "modulePathIgnorePatterns": ["<rootDir>/node_modules/react-native/Libraries/react-native/"],
  "transform": {
    "^.+\\.js$": "babel-jest",
    "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/node_modules/@youi/react-native-youi/__mocks__/fileMock.js"
  },
  "transformIgnorePatterns": [
    "node_modules/(?!(jest-)?react-native|react-clone-referenced-element)"
  ],
  "setupFiles": ["<rootDir>/node_modules/react-native/jest/setup.js"],
  "testEnvironment": "node"
}
